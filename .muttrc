set imap_user = ""
set imap_pass = ""

set smtp_url = ""
set smtp_pass = ""
set from = ""
set realname = ""

set folder = "imaps://imap.gmail.com:993"
set spoolfile = "+INBOX"
set postponed="+[Gmail]/Drafts"

set header_cache=~/.mutt/cache/headers
set message_cachedir=~/.mutt/cache/bodies
set certificate_file=~/.mutt/certificates

set move = no

set mail_check=60
set timeout=15

# html rendering rules
# macro pager \cb ": unset wait_key; set pipe_decode\n|elinks\n: set wait_key; unset pipe_decode\n" "call elinks to extract URLs out of a message"

set sort=date

bind index G imap-fetch-mail
